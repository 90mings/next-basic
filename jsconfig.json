{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      // "@/*": ["./src/*"],
      "@/images/*": ["/public/images/*"],
      "@/svgs/*": ["/public/svgs/*"],
      "@/styles/g/*": ["/src/styles/*"],
      "@/services/*": ["/src/services/*"],
      "@/components": ["/src/components/index"],
      "@/hooks/*": ["/src/hooks/*"],
      "@/stores/*": ["/src/stores/*"],
      "@/utils": ["/src/utils/index"],
      "@/constants/*": ["/src/constants/*"]
    }
  },
  "include": ["src/**/*", "**/*.jsx", "**/*.js"],
  "exclude": ["node_modules"]
}
